@{
    ViewBag.Title = "Google News: ICO Crowdsale";
}
<br />
<h2>Google News: ICO Crowdsale</h2>
<table class="table table-hover">
    <tbody>
        @if (ViewBag.RSSFeed != null)
        {
            foreach (var item in ViewBag.RSSFeed)
            {
                <tr>
                    @*<td>@item.Title</td>
                    <td><a href="@item.Link">@item.Link</a></td>*@
                    <td>@Html.Raw(item.Description) <br/> <div class="text-right pull-right">@DateTime.Parse(item.PubDate).ToShortDateString()</div></td>
                </tr>
            }
        }
    </tbody>
</table>  